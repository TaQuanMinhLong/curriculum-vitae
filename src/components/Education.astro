---
import type { EducationTypes } from "~/types";
interface Props {
  education: EducationTypes;
}

const { education } = Astro.props;
---

<div class="ml-5 mt-6 flex flex-col gap-1 leading-[1em]">
  <h4 class="font-medium uppercase">{education.location}</h4>
  <h5 class="text-sm font-light">{education.name}</h5>
  <h6 class="text-xs font-light">
    {`${education.startDate} - ${education.endDate}`}
  </h6>
</div>
